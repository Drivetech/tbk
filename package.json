{
  "name": "tbk",
  "version": "0.0.1",
  "description": "NodeJS implementation of Transbank's Webpay protocol.",
  "main": "lib",
  "scripts": {
    "prepublish": "eslint .",
    "test": "istanbul cover _mocha",
    "coveralls": "coveralls < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lgaticaq/tbk.git"
  },
  "keywords": [
    "tbk",
    "payment",
    "webpay",
    "transbank"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/tbk/issues"
  },
  "homepage": "https://github.com/lgaticaq/tbk#readme",
  "dependencies": {
    "async": "^2.0.0-rc.5",
    "mathjs": "^3.2.1",
    "request": "^2.72.0",
    "ursa": "^0.9.4"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codeclimate-test-reporter": "0.5.0",
    "coveralls": "2.13.1",
    "eslint": "4.7.1",
    "istanbul": "0.4.5",
    "mocha": "3.5.3"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        2,
        2
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "quotes": [
        2,
        "single"
      ],
      "semi": [
        2,
        "always"
      ]
    }
  },
  "tbkVersionKcc": "6.0"
}
